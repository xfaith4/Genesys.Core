{
  "$schema": "./schema/genesys-core.catalog.schema.json",
  "version": "0.2.0",
  "datasets": {
    "audit-logs": {
      "description": "Sample audit logs dataset.",
      "endpoint": "audits.query.submit",
      "itemsPath": "$.results",
      "paging": {
        "profile": "transactionResults"
      },
      "retry": {
        "profile": "rateLimitAware"
      },
      "transaction": {
        "profile": "auditTransaction"
      }
    }
  },
  "endpoints": [
    {
      "key": "audits.query.submit",
      "method": "POST",
      "path": "/api/v2/audits/query",
      "itemsPath": "$.results",
      "paging": {
        "profile": "transactionResults"
      },
      "retry": {
        "profile": "rateLimitAware"
      },
      "transaction": {
        "profile": "auditTransaction"
      }
    }
  ]
}
